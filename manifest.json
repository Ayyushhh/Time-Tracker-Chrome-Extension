{
    "manifest_version": 3,
    "name": "Time Tracker",
    "version": "1.0",
    "description": "Track the time you spend on websites.",
    "permissions": ["activeTab", "storage", "tabs"],
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": ""
    },
    "options_page": "options.html"
  }